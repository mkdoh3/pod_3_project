<%= form_for @patient do |f| %>

<%= f.label      :name %> <br />
<%= f.text_field :name %>  <br />
<%= f.label      :age %>  <br />
<%= f.text_field :age %>  <br />
<%= f.label      :phone_number %> <br />
<%= f.text_field :phone_number %> <br />
<%= f.label      :address %>  <br />
<%= f.text_field :address %>  <br />

<%= f.submit %> <br />
<% end %> 